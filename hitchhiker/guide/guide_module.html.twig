{% macro guide_item(item, firstone, firsttwo) %}
   {% import "/thumb.html.twig" as thumbBuilder %}
   
   {% set firstItem = firstone and firsttwo %}
   {% set href = item.href %}
   {% set title = item.title %}
   {% set id = item.id %}
   {% set systemIcon = item.systemIcon %}
   {% set thumb = item.thumb %}
   {% set selected = item.selected %}
   
   <li class="vve-check guide-channel" id="{{ id }}-guide-item">
        <a class="guide-item {{ firstItem ? 'narrow-item' }} yt-uix-sessionlink yt-valign spf-link {{ item.selected ? 'guide-item-selected' }}"
    href="{{ href }}"
    title="{{ title }}"
    data-sessionlink=""
    data-channel-id="{{ id }}"
  >
    <span class="yt-valign-container">
      {% if thumb %}
         <span class="thumb">
            {{ thumbBuilder.thumb(thumb, "18", "square") }}
         </span>
      {% elseif systemIcon %}
         <span class="thumb {{ systemIcon }}-icon">
           <img src="//s.ytimg.com/yts/img/pixel-vfl3z5WfW.gif" alt="">
         </span>
      {% endif %}{{ '    ' }}
      <span class="display-name no-count">
        <span>{{ title }}</span>
      </span>
    </span>

  </a>

  </li>
{% endmacro %}

{% macro guide_subscriptions_section(section, separator, first) %}
   {% set title = section.title %}
   {% set type = section.type %}
   {% set items = section.items %}

   <li id="guide-subscriptions-section" class="guide-section vve-check" data-sessionlink="">
      {% if title %}
         <h3>{{ title }}</h3>
      {% endif %}
      <div id="guide-subs-footer-container">
         <div id="guide-subscriptions-container">
            <div class="guide-channels-content yt-scrollbar">
               <ul id="guide-channels" class="guide-channels-list guide-item-container yt-uix-scroller filter-has-matches yt-uix-tdl">
                  {% for item in items %}
                     {{ _self.guide_item(item, first, loop.first) }}
                  {% endfor %}
               </ul>
            </div>
         </div>
      </div>
      {% if separator %}
         <hr class="guide-section-separator">
      {% endif %}
   </li>
{% endmacro %}

{% macro guide_section(section, separator, first) %}
   {% set title = section.title %}
   {% set items = section.items %}

   <li class="guide-section vve-check" data-sessionlink="">
      <div class="guide-item-container personal-item">
         {% if title %}
            <h3>{{ title }}</h3>
         {% endif %}
         <ul class="guide-user-links yt-box">
            {% for item in items %}
               {{ _self.guide_item(item, first, loop.first) }}
            {% endfor %}
         </ul>
      </div>
      {% if separator %}
         <hr class="guide-section-separator">
      {% endif %}
   </li>
{% endmacro %}

